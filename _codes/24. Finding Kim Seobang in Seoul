using System;

public class Solution 
{
    public string solution(string[] seoul) 
    {
        int num = -1;
        for (int i = 0; i < seoul.Length; i++) 
        {
            if (seoul[i].Equals("Kim")) {
                num = i;
                break;
            }
        }

        string answer = $"김서방은 {num}에 있다";
        return answer;
    }
}
